@model List<Resto>
@{
    Layout = "~/Views/Shared/_Layout2.cshtml";
    ViewBag.Title = "Liste Restos";
}


<table>
    <tr>
        <th>Nom</th>
        <th>Téléphone</th>
    </tr>
    @foreach (var resto in Model)
    {
        <tr>
            <td>@resto.Nom</td>
            <td>@resto.Telephone</td>
        </tr>
    }
</table>